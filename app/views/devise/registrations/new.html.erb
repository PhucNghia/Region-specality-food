<div class="limiter limiter-register">
  <div class="container-login100" style="background-image: url('https://cdn.dealtoday.vn/img/s630x420/439cebed8dfd4cee93be5895fc3e3fba.JPG?sign=H1NxW1olFYbnS6Y8g9eovw'); ">
    <div class="container">
      <div class="row main register-main">
        <div class="main-login main-center">
          <h3 class="text-center"><%= t ".sign_up" %></h3>
          <%= form_for(resource, as: resource_name, url: registration_path(resource_name), class: "register-form") do |f| %>
            <%#= devise_error_messages! %>
            <%= render "form", f: f %>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>
